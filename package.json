{
    "name": "application",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "start:dev": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./bin/www",
        "db:create:migration": "npx sequelize migration:generate --name ",
        "db:migrate": "npx sequelize db:migrate",
        "db:rollback": "npx sequelize db:migrate:undo",
        "postinstall": "npx sequelize db:migrate",
        "build:css": "npx tailwindcss -i ./public/stylesheets/tailwind.css -o ./public/dist/output.css --watch"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^16.0.0",
        "express": "~4.16.1",
        "express-handlebars": "^6.0.3",
        "hbs": "^4.2.0",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1",
        "nodemon": "^2.0.15",
        "pg-promise": "^10.11.1",
        "sequelize": "^6.17.0",
        "sequelize-cli": "^6.4.1", 
        "tailwindcss": "^3.0.23"
    },
    "devDependencies": {
        "tailwindcss": "^3.0.23"
    }
}
